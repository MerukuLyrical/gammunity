    <%= form_with(model: @tpic, url: tpics_path, local: true) do |form| %>
      <%= form.text_field :title, placeholder: "タイトル" %>
      <%= form.text_field :tag_name, placeholder: "タグ" %>
      <%= form.text_area :text, placeholder: "本文", rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>
